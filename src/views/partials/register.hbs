  <h1>Skate Park</h1>

    <div class="py-4">
      <h2>Registro</h2>
      <hr>

      <form id="register_form" action="" method="" enctype="" > 
        <div class="form-group row w-50 m-auto">
          <div class="form-group col-12 col-sm-6">
            <label>Email</label>
            <input id="email_register" class="form-control m-auto" type="email" name="email"/>
          </div>
          <div class="form-group col-12 col-sm-6">
            <label>Nombre</label>
            <input id="name_register"class="form-control m-auto" type="text" name="name"/>
          </div>
          <div class="form-group col-12 col-sm-6">
            <label>Password</label>
            <input id="password" class="form-control m-auto" type="password" name="password"/>
          </div>
          <div class="form-group col-12 col-sm-6">
            <label>Repita la password</label>
            <input id="password_confirm" class="form-control m-auto" type="password" name="password_confirm"/>
          </div>
          <div class="form-group col-12 col-sm-6">
            <label>Años de experiencia</label>
            <input id="anos_experiencia_register"class="form-control m-auto" type="number" name="anos_experiencia"/>
          </div>
          <div class="form-group col-12 col-sm-6">
            <label>Especialidad</label>
            <input id="especialidad_register"class="form-control m-auto" type="text" name="especialidad"/>
          </div>          
          <div class="form-group col-12 mt-3">
            <label>Foto de perfil</label>
            <input id="foto_register" type="file" name="foto"/>
          </div>
        </div>
        <button class="btn btn-primary my-3">Registrarme</button>
        <p><a href="/auth/login"> Iniciar sesión</a></p>
      </form>
    </div>
    <script type="module"src="/js/register_form.js"></script>


